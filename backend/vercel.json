{
  "version": 2,
  "buildCommand": "cp package.vercel.json package.json && npm install && npm run vercel-build",
  "outputDirectory": ".",
  "builds": [
    {
      "src": "dist/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}