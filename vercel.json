{
  "root": "frontend",
  "buildCommand": "cd .. && npm run build:shared && cd frontend && npm run build",
  "outputDirectory": "dist",
  "installCommand": "cd .. && npm install --workspaces --legacy-peer-deps",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}